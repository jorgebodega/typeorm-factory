    "@typescript-eslint/eslint-plugin": "5.38.1",
    "@typescript-eslint/parser": "5.38.1",
    "eslint": "8.24.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "jest": "29.1.2",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
    "sqlite3": "5.1.1",
    "ts-jest": "29.0.3",
