{
  "name": "@jorgebodega/typeorm-factory",
  "version": "1.1.0",
  "description": "ðŸŒ± A delightful way to use factories in your code.",
  "license": "MIT",
  "author": "Jorge Bodega <jorge.bodega.f@gmail.com> (https://github.com/jorgebodega)",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/jorgebodega/typeorm-factory.git"
  },
  "scripts": {
    "build": "tsc --project ./tsconfig.build.json",
    "checks": "pnpm format:ci && pnpm lint:ci && pnpm typecheck",
    "format": "prettier --write \"{src,test}/**/*.ts\"",
    "format:ci": "prettier --check \"{src,test}/**/*.ts\"",
    "lint": "eslint \"{src,test}/**/*.ts\"",
    "lint:fix": "pnpm lint --fix",
    "lint:ci": "pnpm lint",
    "prebuild": "rimraf dist",
    "schema:drop": "ts-node ./node_modules/typeorm/cli.js schema:drop",
    "schema:log": "ts-node ./node_modules/typeorm/cli.js schema:log",
    "schema:sync": "ts-node ./node_modules/typeorm/cli.js schema:sync",
    "test": "jest",
    "test:ci": "jest --silent",
    "test:cov": "jest --coverage --silent",
    "test:watch": "jest --watch",
    "typecheck": "tsc --noEmit"
  },
  "engines": {
    "node": ">=14"
  },
  "devDependencies": {
    "@faker-js/faker": "7.3.0",
    "@tsconfig/node16": "1.0.3",
    "@types/jest": "28.1.4",
    "@types/node": "18.6.1",
    "@typescript-eslint/eslint-plugin": "5.30.5",
    "@typescript-eslint/parser": "5.30.5",
    "eslint": "8.20.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "jest": "28.1.3",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
    "sqlite3": "5.0.10",
    "ts-jest": "28.0.7",
    "ts-node": "10.9.1",
    "typeorm": "0.3.7",
    "typescript": "4.7.4"
  },
  "peerDependencies": {
    "typeorm": "^0.3.0"
  }
}
